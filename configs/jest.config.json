{
  "rootDir": "../",
  "testRegex": "((\\.|/*.)(test))\\.js?$",
  "testPathIgnorePatterns": [
    "/AniGraph/"
  ],
  "moduleNameMapper": {
    "\\.(css|less)$": "identity-obj-proxy",
